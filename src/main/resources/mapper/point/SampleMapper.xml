<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.kr.kgc.point.batch.mapper.point.SampleMapper">
    <select id="selectSampleData" resultType="map">
        SELECT
            column1 AS "column1"
        FROM POINT_TABLE1
    </select>
    <insert id="insertSampleData" parameterType ="map">
        INSERT INTO POINT_TABLE1 VALUES (#{column1}, #{column2}, SYSDATE, SYSDATE)
    </insert>
</mapper>